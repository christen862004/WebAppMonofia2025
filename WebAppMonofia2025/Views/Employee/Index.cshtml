@model List<Employee>
@{
    ViewData["Title"] = "Index";//Write In ViewData
}

<h1>Index</h1>

<table class="table table-hover table-bordered">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td></td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>
                <a href="/Employee/Edit/@item.Id">Edit</a>
            </td>
        </tr>
    }
</table>